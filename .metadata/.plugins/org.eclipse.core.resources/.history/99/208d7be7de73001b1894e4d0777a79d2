package fundamentos;

public class PrimeiroPrograma {

	public static void main(String[] args) {
		
		// A code sentence finish with ;
		// More one comment...
		// End
		System.out.println("Danilover #01!!!");
		/*
		 *Line 1
		 *Line 2
		 *Line 3
		 */
		System.out.println("Danilover #02!!!");
	}
}
